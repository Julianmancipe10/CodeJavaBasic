
public class ejercice6 {
	public static void main(String[]arg) {
		
		 int num1 = 5;
	        
		 	int num2 = 3;
	        
	        int num3 = 10;
	        
	        int num4 = 2;
	        
	        int num5 = 4;
	        
	        int resultado = num1 * num2 + num3 / num4 - num5;
	        
	         System.out.println("El resultado es: " + resultado);
	}
}
