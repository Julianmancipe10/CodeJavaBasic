
public class Principal {

	public static void main (String [] arg) 
	{
	int x=14; //Delcaracion de inicialicacion de varialble x
	int y=2; //Delcaracion de inicialicacion de varialble y
	
	System.out.println("El valor x =" + x + " Valor de y =" +y ); 
	
	int suma =x+y;
	
	System.out.println("X+Y ="+ suma);
	
	int resta=x-y;
	
	System.out.println("X-Y="+resta);
	
	int multiplicacion=x*y;
	
	System.out.println("X*Y=" + multiplicacion);
	
	int division=x/y;
	
	System.out.print("X/Y="+division);
	
	
	}
}
